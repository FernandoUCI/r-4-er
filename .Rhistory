# creating sample data set
# ex_dates is the string of the dates that contain T and Z
ex_dates <- c("2016-12-07T15:51:23Z", "2016-12-08T15:51:23Z", "2016-12-09T15:51:23Z")
ex_id <- c("ID1", "ID2", "ID3")
ex_stringdate <- as_tibble(cbind(ex_id, ex_dates))
head(ex_stringdate)
# making a new variable, `ex_stringdate$ex_dates2`
# this is where we will store the converted date variables
ex_stringdate$ex_dates2 <- NA
# Removing the ` and Z using `gsub()`
ex_stringdate$ex_dates2 <- gsub("T|Z", " ", ex_stringdate$ex_dates)
# Converting the variable, `ex_stringdate$ex_dates2` to date-time
ex_stringdate$ex_dates2 <- as.POSIXct(ex_stringdate$ex_dates2)
# Checking that the structure is POSIXct
str(ex_stringdate)
